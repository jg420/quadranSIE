details_centrale['lib_type'] = [];  //lib_type
details_centrale['id_cex'] = [];  //id_cex
details_centrale['lib_cex'] = [];  //lib_cex en trigrame ex: LNI
details_centrale['id_direction'] = []; // id_direction

